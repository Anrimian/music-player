<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <exclude domain="external"/>
        <include domain="database" path="configs_database"/>
        <include domain="file" path="playlists"/>
        <include domain="sharedpref" path="settings_preferences.xml"/>
        <include domain="sharedpref" path="theme_preferences.xml"/>
    </cloud-backup>
    <device-transfer>
        <exclude domain="external"/>
        <include domain="database" path="configs_database"/>
        <include domain="file" path="playlists"/>
        <include domain="sharedpref" path="settings_preferences.xml"/>
        <include domain="sharedpref" path="theme_preferences.xml"/>
    </device-transfer>
</data-extraction-rules>